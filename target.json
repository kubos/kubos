{
  "name": "pyboard-gcc",
  "version": "0.0.2",
  "inherits": {
    "stm32f405-gcc": "openkosmosorg/target-stm32f405-gcc#~0.0.1"
  },
  "description": "KubOS build target for the stm32f405rg based PyBoard.",
  "author": "Ryan Plauche <ryan@kubos.co>",
  "homepage": "https://github.com/openkosmosorg/target-pyboard-gcc",
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "keywords": [
    "stm32f405rg",
    "gcc"
  ],
  "similarTo": [
    "stm32f405rg",
    "f405rg",
    "f405",
    "pyboard",
    "stm32f405rg",
    "stm32f405",
    "stm32f4",
    "stm32",
    "st",
    "cortex-m4",
    "armv7-m"
  ],
  "config": {
    "hardware": {
      "externalClock": "8000000",
      "console": {
        "uart": "K_UART6",
        "baudRate": "115200"
      },
      "pins": {
        "LED1": "PA14",
        "LED2": "PA15",
        "LED3": "PA13",
        "LED4": "PB4",
        "USER_BUTTON": "PB3"
      }
    }
  },
  "toolchain": "CMake/toolchain.cmake",
  "debug": [
    "valinor",
    "--target",
    "stm32f405rg",
    "$program"
  ],
  "scripts": {
    "debug": [
      "valinor",
      "--target",
      "stm32f405rg",
      "$program"
    ],
    "test": [
      "mbed_test_wrapper",
      "--target",
      "STM32F4-DISCO",
      "$program"
    ]
  }
}
